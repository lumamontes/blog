{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
  "globals": {
    "React": "readonly",
    "globalThis": false // means it is not writeable
  },
  "rules": {
    "no-unused-vars": [
      1,
      {
        "ignoreRestSiblings": true,
        "destructuredArrayIgnorePattern": "[A-Z]",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
